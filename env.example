# React App Environment Variables

# Stripe Configuration
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
REACT_APP_STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here

# Cyfe Dashboard Configuration
REACT_APP_CYFE_BASE_URL=https://app.cyfe.com
REACT_APP_CYFE_API_KEY=your_cyfe_api_key_here
REACT_APP_CYFE_WHITELABEL_DOMAIN=https://dashboards.yourdomain.com

# SSO Configuration
REACT_APP_SSO_SECRET_KEY=your_sso_secret_key_here
REACT_APP_SSO_TOKEN_EXPIRY=86400

# GoHighLevel Integration
REACT_APP_GHL_API_KEY=your_ghl_api_key_here
REACT_APP_GHL_WEBHOOK_SECRET=your_ghl_webhook_secret_here
REACT_APP_GHL_LOCATION_ID=your_ghl_location_id_here

# GoHighLevel OAuth (for user authentication)
REACT_APP_GHL_OAUTH_CLIENT_ID=your_oauth_client_id_here
REACT_APP_GHL_OAUTH_CLIENT_SECRET=your_oauth_client_secret_here
REACT_APP_GHL_OAUTH_REDIRECT_URI=http://localhost:3000/auth/callback

# Application Configuration
REACT_APP_API_BASE_URL=https://api.yourdomain.com
REACT_APP_APP_NAME=SiteOptz Scraping

# Dashboard Configuration
REACT_APP_MAX_FREE_DASHBOARDS=1
REACT_APP_MAX_STARTER_DASHBOARDS=2
REACT_APP_MAX_PRO_DASHBOARDS=4
REACT_APP_MAX_ENTERPRISE_DASHBOARDS=-1

# Phone Validation APIs
REACT_APP_TWILIO_ACCOUNT_SID=your_twilio_account_sid
REACT_APP_TWILIO_AUTH_TOKEN=your_twilio_auth_token
REACT_APP_PHONE_VALIDATION_API_KEY=your_numverify_or_abstract_api_key

# AI-Powered SEO Service APIs
REACT_APP_OPENAI_API_KEY=sk-your_openai_api_key_here
REACT_APP_ANTHROPIC_API_KEY=sk-ant-your_anthropic_api_key_here
REACT_APP_PERPLEXITY_API_KEY=pplx-your_perplexity_api_key_here
REACT_APP_AHREFS_API_KEY=your_ahrefs_api_key_here
REACT_APP_SEMRUSH_API_KEY=your_semrush_api_key_here
REACT_APP_FIRECRAWL_API_KEY=fc-your_firecrawl_api_key_here

# Feature Flags
REACT_APP_ENABLE_CYFE_INTEGRATION=true
REACT_APP_ENABLE_STRIPE_INTEGRATION=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_PHONE_VALIDATION=true
REACT_APP_ENABLE_AI_SEO_SERVICE=true

# Development/Production
NODE_ENV=development
